{
  "name": "mab05k/oanda-client",
  "description": "PHP Client for Oanda v20 REST-API",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.1.3|^8.2",
    "ext-json": "*",
    "brick/math": "^0.8|^0.10|^0.11",
    "brick/money": "^0.4|^0.6|^0.8",
    "jms/serializer-bundle": "^3.5|^4.0|^5.0",
    "php-http/guzzle6-adapter": "^2.0",
    "php-http/httplug-bundle": "^1.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14|^3.0",
    "http-interop/http-factory-guzzle": "^1.1",
    "monolog/monolog": "^1.24",
    "phake/phake": "^3.0|^4.0",
    "php-http/mock-client": "^1.2",
    "phpunit/phpunit": "^7.0|^9.0|^10.0",
    "symfony/browser-kit": "^4.2|^5.4|^6.0",
    "symfony/dotenv": "^4.2|^5.4|^6.0",
    "symfony/monolog-bundle": "^3.3",
    "symfony/yaml": "^4.2|^5.4|^6.0"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "php-http/discovery": true
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mab05k\\OandaClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mab05k\\OandaClient\\Tests\\": "tests/",
      "App\\": "tests/Fixtures/harness/src/"
    }
  }
}
